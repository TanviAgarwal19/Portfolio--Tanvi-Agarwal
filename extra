    font-size: 70px;
    font-family: arial black;  
  }


  .text, .slideDown, .slideUp {
    position: relative;
    font-family: tahoma;
    font-size: 35px;
    opacity:0;
  }
  
  .text {
    top:40px;
    font-weight: bold;
    animation: slideUp ease .4s forwards;
  }
  
  .slideDown {
    top:-20px;
    left:5px;
    animation: slideDown ease .10s forwards .12s;
  }
  
  .slideUp {
    top:20px;
    left:10px;
    animation: slideUp ease .12s forwards 6.2s;
  }

  .b, .o, .u, .n, .c, .e {
    color: #ffffff;
    text-align: center;
  }
  @keyframes bounce {
    0%   { transform: scale(1,1) translateY(0); }
    10%  { transform: scale(1.1,.9) translateY(0); }
    30%  { transform: scale(.9,1.1)   translateY(-55px);}
    50%  { transform: scale(1.05,.95) translateY(0); }
    58%  { transform: scale(1,1) translateY(-7px); }
    65%  { transform: scale(1,1) translateY(0);}
    100% { transform: scale(1,1) translateY(0);}
}

.b {
animation: bounce 1s ease infinite;
}

.o {
animation: bounce 1s ease infinite .1s;
}

.u {
animation: bounce 1s ease infinite .2s;
}

.n {
animation: bounce 1s ease infinite .3s;
}

.c {
