  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-transition: all 0.4s ease-in-out;
  transition: all 0.4s ease-in-out;
}

figure.overlay img {
  position: relative;
  opacity: 1;
}

figure.overlay figcaption {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
}


figure.overlay h2 span {
  font-weight: 100;
}

